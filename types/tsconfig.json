{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es5", "es2015", "dom"],
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noEmit": true,
    "strictFunctionTypes": false,
    "baseUrl": ".",
