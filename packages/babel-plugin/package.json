{
  "name": "@loadable/babel-plugin",
  "description": "Babel plugin for loadable (required for SSR).",
  "version": "5.10.3",
  "main": "lib/index.js",
  "repository": "git@github.com:smooth-code/loadable-components.git",
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "loadable"
  ],
  "engines": {
    "node": ">=8"
  },
